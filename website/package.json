    "@tabler/icons-react": "^3.35.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "deepmerge": "^4.3.1",
    "gsap": "^3.13.0",
    "lenis": "^1.3.13",
    "lucide-react": "^0.552.0",
    "mathjs": "^15.0.0",
