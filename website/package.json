{
  "name": "@next-rich-tpl/with-i18n-routing",
  "version": "16.0.1",
  "private": true,
  "scripts": {
    "dev": "pnpm run mergeTranslations && next dev",
    "build": "pnpm run mergeTranslations && next build",
    "start": "next start",
    "lint": "eslint",
    "mergeTranslations": "node ./_job/mergeTranslations.js",
    "format": "pnpm dlx prettier . --write",
    "lh:run": "pnpm lighthouse http://localhost:3000 --output html --output-path=output/lh-report.html --chrome-flags='--headless'",
    "lh:view": "node ./scripts/open.mjs ./output/lh-report.html",
    "al:build": "cross-env pnpm next build --webpack",
    "al:run": "pnpm source-map-explorer \".next/static/chunks/*.js\"",
    "al:view": "node ./scripts/open.mjs .next/analyze/client.html"
  },
  "license": "MIT",
